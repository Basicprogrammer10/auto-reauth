{
  "schemaVersion": 1,
  "id": "auto-reauth",
  "version": "${version}",
  "name": "auto-reauth",
  "description": "Automatically re-authenticates your session when it expires",
  "authors": ["Connor Slade"],
  "contact": {
    "repo": "https://github.com/connorslade/auth-reauth"
  },
  "license": "MIT",
  "environment": "client",
  "entrypoints": {
    "client": [
      "com.connorcode.autoreauth.AutoReauth"
    ],
    "modmenu": [
      "com.connorcode.autoreauth.ModMenu"
    ]
  },
  "mixins": [
    "auto-reauth.mixins.json"
  ],
  "accessWidener": "auto-reauth.accesswidener",
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  },
  "custom": {
    "modmenu": {
      "links": {
        "Github": "https://github.com/Basicprogrammer10/auto-reauth",
        "My Website": "https://connorcode.com"
      }
    }
  }
}
